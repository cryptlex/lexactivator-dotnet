<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>Cryptlex.LexActivator</id>
    <version>3.31.3</version>
        <title>Cryptlex Licensing Library</title>
        <authors>Cryptlex</authors>
        <owners>Cryptlex LLP</owners>
        <projectUrl>https://cryptlex.com</projectUrl>
        <repository type="git" url="https://github.com/cryptlex/lexactivator-dotnet" />
        <license type="expression">MIT</license>
        <language>en-US</language>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <summary>LexActivator API wrapper for .NET (licensing library).</summary>
        <description>Cryptlex lets you license your software apps effortlessly. You can easily generate license keys using the REST API or Dashboard and validate the license keys in your software apps using LexActivator (Cryptlex client library).</description>
        <dependencies>
          <group targetFramework="net45">
            <dependency id="Newtonsoft.Json" version="12.0.3" />
          </group>
          <group targetFramework="netstandard1.1">
            <dependency id="Newtonsoft.Json" version="12.0.3" />
          </group>
        </dependencies>
        <copyright>Copyright (c) 2023 Cryptlex LLP </copyright>
        <tags>lexactivator cryptlex licensing-library</tags>
        <references>
            <reference file="Cryptlex.LexActivator.dll" />
        </references>
    </metadata>
